{
  "ConnectionStrings": {
    "Default": "${ConnectionStrings__Default}"
  },
  "Settings": {
    "Passwords": {
      "AdditionalKey": "${Settings__Passwords__AdditionalKey}"
    }
  },
  "Jwt": {
    "Key": "${Jwt__Key}",
    "Issuer": "${Jwt__Issuer}",
    "Audience": "${Jwt__Audience}",
    "ExpiresInHours": "${Jwt__ExpiresInHours}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
